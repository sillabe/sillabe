{
    "name": "@sillabe/markdown-posts-plugin",
    "version": "0.1.5",
    "main": "dist/lib/index.js",
    "typings": "dist/types/index.d.ts",
    "files": [
        "dist"
    ],
    "license": "MIT",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "tsc --module commonjs",
